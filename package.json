    "typescript": "^3.7.4",
    "vue-template-compiler": "^2.6.11",
    "yaml-lint": "^1.2.4",
    "markdownlint-cli": "^0.23.1",
    "remark-cli": "^8.0.0",
    "remark-lint-no-dead-urls": "^1.0.2",
    "remark-preset-lint-consistent": "^3.0.0",
    "remark-validate-links": "^10.0.0"
